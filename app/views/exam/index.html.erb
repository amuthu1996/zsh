<h1>Exam#index</h1>
<p>Find me in app/views/exam/index.html.erb</p>


<table>
  <thead>
    <tr>
      <th>id</th>
      <th>Tname</th>
      <th>Doj</th>
      <th>Gender</th>
      <th>Experience</th>
      <th>Contact</th>
      <th>class</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cls_teacher.each do |teacher| %>
      <tr>
        <td><%= teacher.id %></td>
        <td><%= teacher.tname %></td>
        <td><%= teacher.doj %></td>
        <td><%= teacher.gender %></td>
        <td><%= teacher.exp %></td>
        <td><%= teacher.ph %></td>
        <td><%= teacher.cl.std %></td>
        <td><%= link_to 'Show', teacher %></td>
        <td><%= link_to 'Edit', edit_teacher_path(teacher) %></td>
        <td><%= link_to 'Destroy', teacher, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>